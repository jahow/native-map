{
  "name": "native-map",
  "version": "0.1.0",
  "description": "A native web component used to render interactive maps",
  "main": "./dist/index.js",
  "browser": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
