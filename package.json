    "@types/jest": "^29.2.2",
    "babel-loader": "^8.2.3",
    "esbuild": "^0.15.13",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "2.7.1",
    "regenerator-runtime": "^0.13.10",
